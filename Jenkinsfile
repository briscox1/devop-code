pipeline {
  agent any
  stages {
    stage('build stage'){
      steps {
        echo " build steps"
        sleep 10
      }
    }
    stage('test'){
      steps {
        echo " testing 1 2 3"
        sleep 10
      }
    }
    stage('docker'){
      steps {
        echo " docker 1 2 3"
        sleep 10
      }
    }
  }
}
